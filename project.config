[project]
	description = A cross-platform Qt IDE
	state = active
[receive]
[submit]
	action = cherry pick
[access "refs/heads/*"]
	create = group Local Admins: Qt Creator
	push = group Local Admins: Qt Creator
[access "refs/tags/*"]
	pushTag = +force group Local Admins: Qt Creator
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
[access]
	inheritFrom = All-Non-CI-Projects
[access "refs/heads/wip/vcproj"]
	label-Code-Review = -2..+2 group Local Approvers: Qt Creator wip/vcproj
	submit = group Local Approvers: Qt Creator wip/vcproj
[access "refs/heads/wip/qbs"]
	push = +force group Approvers
[access "refs/heads/master"]
	submit = group Local Admins: Qt Creator
	exclusiveGroupPermissions = submit
[access "refs/*"]
	owner = group Local Admins: Qt Creator
	viewDrafts = group Push Masters
[access "refs/for/refs/heads/3.0"]
	exclusiveGroupPermissions = push
[cherryPick "labelFooters"]
	Sanity-Review = true
